# ==============================================================================
# 站点配置 (Site Configuration)
# ==============================================================================

# 网站运营起始年份，用于页脚版权显示
VITE_SITE_START_YEAR=2024
# 网站运营起始日期 (格式: YYYY-MM-DD)，用于计算运行时间
VITE_SITE_START_DATE=2024-01-01

# 站点名称
VITE_SITE_NAME=Your Site Name

# 站长出生日期 (格式: YYYY-MM-DD)，用于计算年龄
VITE_USER_BIRTH_DATE=2000-01-01

# 个人信息 (Personal Information)
# ==============================================================================

# 站长角色/职业 (ZH/EN)
VITE_USER_ROLE_ZH=软件工程师
VITE_USER_ROLE_EN=Software Engineer

# 站长个性签名 (ZH/EN)
VITE_USER_QUOTE_ZH=“你的座右铭”
VITE_USER_QUOTE_EN="Your Motto"

# GitHub 用户名 (用于获取仓库列表、生成头像和链接)
VITE_GITHUB_USERNAME=your_github_username

# 头像 URL (留空则自动使用 GitHub 头像)
VITE_AVATAR_URL=

# ==============================================================================
# 网站链接 (Site URLs)
# ==============================================================================

# 主站地址
VITE_SITE_URL=https://example.com

# 博客地址 (用于博客页面 iframe)
VITE_BLOG_URL=https://blog.example.com

# 自动化部署输出路径 (可选)
# VITE_BLOG_SOURCE_DIR=/path/to/source
# VITE_BLOG_OUTPUT_DIR=/path/to/output
# VITE_STATIC_OUTPUT_DIR=/path/to/static

# ==============================================================================
# 开源仓库 (Repository)
# ==============================================================================

# 项目仓库地址
VITE_REPO_URL=https://github.com/your_username/your_repo

# 项目仓库名称 (用于页脚显示)
VITE_REPO_NAME=Your Repository Name

# ==============================================================================
# SEO 配置 (SEO Configuration)
# ==============================================================================

# 作者名称
VITE_SEO_AUTHOR=Your Name

# 网站描述
VITE_SEO_DESCRIPTION=Your site description.

# SEO 关键词 (逗号分隔)
VITE_SEO_KEYWORDS=Keyword1, Keyword2

# Twitter 用户名 (不含 @)
VITE_TWITTER_ID=your_twitter_id

# ==============================================================================
# 第三方服务 (Third Party Services)
# ==============================================================================

# 壁纸背景 API 地址
VITE_WALLPAPER_API=https://api.example.com/wallpaper

# 高德地图 (AMap) 配置
VITE_AMAP_KEY=your_amap_key_here
VITE_AMAP_SECURITY_CODE=your_amap_security_code_here


